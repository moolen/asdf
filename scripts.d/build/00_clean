#!/bin/bash
set -e

function startCommand {
    rm -rf dist
    docker rm -f smcl_build >/dev/null 2>&1 || true
}

function help {
    cat << "EOF"
Usage: ./kitt build 00_clean
EOF
}

function init {
    echo $@
    # shellcheck disable=SC2194
    case "$1" in
        --help)
            help
            exit 0
            ;;
        *)
            startCommand
            exit 0
    esac
}
init $@

